{
  "name": "vitest-zest",
  "version": "0.4.5",
  "license": "MIT",
  "author": "Denis Olsem",
  "homepage": "https://github.com/dolsem/vitest-zest#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dolsem/vitest-zest.git"
  },
  "bugs": {
    "url": "https://github.com/dolsem/vitest-zest/issues"
  },
  "keywords": [
    "vitest",
    "test",
    "jest"
  ],
  "engines": {
    "node": ">= v16.20.0"
  },
  "source": "src/index.ts",
  "main": "dist/main/index.js",
  "module": "dist/vitest-zest.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.js",
        "default": "./dist/vitest-zest.js"
      }
    }
  },
  "targets": {
    "main": {
      "optimize": false
    }
  },
  "scripts": {
    "prepack": "vitest --run && npm run build",
    "dev": "parcel watch",
    "tsc": "tsc --noEmit",
    "build": "parcel build",
    "test": "vitest"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "peerDependencies": {
    "vitest": "^2.0.5"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.10.3",
    "@parcel/transformer-typescript-types": "^2.10.3",
    "@types/node": "^22.0.1",
    "parcel": "^2.10.3",
    "typescript": "^4.9.5"
  }
}
